<!-- views/urls_index.ejs -->

<!DOCTYPE html>
<html lang="en">

<head>
  <title>TinyApp</title>
</head>


<body>
    <!-- Add your EJS code here -->

  <header>
    <% include ./partials/_header%>
  </header>

  <main>
    <h1>URL's</h2>
    <p>
      <% for (var shortURLS in urls) { %>
        <li>
          <form method="POST" action="/urls/<%= shortURLS %>/delete">
            <a href='#'><%= shortURLS %></a> > <%= urls[shortURLS] %>
            <input type="submit" value="Delete">
          </form>
        </li>
      <% }; %>
    </p>

  </main>
</body>

</html>